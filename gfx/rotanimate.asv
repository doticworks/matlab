function rotanimate(z,f,tseq)
    told=0;
    for t=tseq
        rotnow=rotchain(f,t);
        
        plotzchain(z(1,:).*rotnow,'-oy');
        plotzchain(z(2,:).*rotnow,'-oy');
        plotzchain(z(3,:).*rotnow);
        title(t);
        pause((t-told)*10);
        told=t;
    end
end
